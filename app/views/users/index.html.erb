<% provide(:title, 'Users') %>
<h1>All users</h1>

<%= will_paginate %>

<ul class="users">
	<% @users.each do |user| %>
		<li>
			<% if false %>
			<%= gravatar_for user %><br/>
			<% end %>
			<%= image_tag user.photo.url(:thumb) %>
			<%= link_to user.name, user %>
		</li>
	<% end %>
</ul>

<%= will_paginate %>